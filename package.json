{
  "name": "Split_It",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "clean": "npm run clean:cache && npm run clean:android && npm run clean:ios && npm run clean:node",
    "clean:cache": "react-native start --reset-cache",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "clean:ios": "cd ios && rm -rf build && rm -rf Pods && rm -rf Podfile.lock && pod install && cd ..",
    "clean:node": "rm -rf node_modules && npm install",
    "clean:derived": "rm -rf ~/Library/Developer/Xcode/DerivedData",
    "clean:metro": "rm -rf /tmp/metro-* && rm -rf /tmp/react-*",
    "clean:watchman": "watchman watch-del-all",
    "clean:all": "npm run clean:watchman && npm run clean:metro && npm run clean:derived && npm run clean:node && npm run clean:android && npm run clean:ios",
    "pod:install": "cd ios && pod install && cd ..",
    "pod:update": "cd ios && pod update && cd ..",
    "pod:deintegrate": "cd ios && pod deintegrate && cd ..",
    "gradle:clean": "cd android && ./gradlew clean && cd ..",
    "gradle:build": "cd android && ./gradlew assembleDebug && cd ..",
    "reset": "npm run clean:all && npm run pod:install && npm start"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.2",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/stack": "^7.4.2",
    "react": "18.3.1",
    "react-native": "0.76.9",
    "react-native-async-storage": "^0.0.1",
    "react-native-gesture-handler": "^2.27.1",
    "react-native-reanimated": "^3.18.0",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.11.1",
    "react-native-svg": "^15.12.0",
    "react-native-vector-icons": "^10.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.76.9",
    "@react-native/eslint-config": "0.76.9",
    "@react-native/metro-config": "0.76.9",
    "@react-native/typescript-config": "0.76.9",
    "@types/react": "^18.2.6",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
